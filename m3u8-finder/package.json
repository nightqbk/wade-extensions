{
  "name": "wade-m3u8-finder",
  "displayName": "Wade m3u8 finder",
  "version": "0.0.1",
  "description": "A basic Plasmo extension.",
  "author": "wcai",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/messaging": "^0.1.0",
    "@plasmohq/storage": "^1.0.4",
    "bip39": "^3.0.4",
    "daisyui": "^2.50.0",
    "plasmo": "0.64.5",
    "random": "^4.1.0",
    "react": "18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.1",
    "@types/chrome": "0.0.210",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.21",
    "prettier": "2.8.3",
    "tailwindcss": "^3.2.6",
    "typescript": "4.9.4"
  },
  "manifest": {
    "permissions": [
      "declarativeNetRequest",
      "declarativeNetRequestWithHostAccess",
      "declarativeNetRequestFeedback",
      "tabs",
      "activeTab"
    ],
    "host_permissions": [
      "https://*/*"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "tabs/config-page.html"
        ]
      }
    ]
  }
}